{
  "$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
  "linter": {
    "enabled": true,
    "rules": {
      "recommended": false
    }
  },
  "css": {
    "linter": {
      "enabled": true
    }
  },
  "overrides": [
    {
      "include": ["**/*.jsx", "**/*.tsx", "**/*.js", "**/*.ts"],
      "linter": {
        "rules": {}
      }
    },
    {
      "include": ["**/*.css", "**/*.scss", "**/*.sass"],
      "linter": {
        "rules": {}
      }
    }
  ]
}

